{
    //Do not modify this file!
    "$schema": "http://json-schema.org/schema#",
    "type":"object",
    "properties": {
        "releaseList":{
            "type":"array",
            "items":{
                "type":"object",
                "properties": {
                    "title":{
                        "type":"string"
                    },
                    "albumArtist":{
                        "type":"string"
                    },
                    "date":{
                        "type":"integer"
                    },
                    "genre":{
                        "type":"string"
                    },
                    "trackTitles":{
                        "type":"array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "trackArtists":{
                        "type":"array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "title","albumArtist","trackTitles"
                ]
            }
        }
    },
    "releaseList": [
        "releases"
    ]
}